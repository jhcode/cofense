{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListGetPut",
      "Principal": "*",
      "Effect":"Allow",
         "Action":[
            "s3:PutObject",
            "s3:PutObjectAcl",
            "s3:GetObject",
            "s3:GetObjectAcl"
         ],
      "Resource": "arn:aws:s3:::${bucket_name}/*",
    }
  ]
}
